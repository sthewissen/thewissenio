  "scripts": {
    "dev": "cross-env NODE_ENV=development THEMES=Main webpack --hide-modules",
    "prod": "cross-env NODE_ENV=production THEMES=Main webpack --hide-modules",
    "watch": "cross-env NODE_ENV=development THEMES=Main webpack --hide-modules --watch",
    "build": "cross-env NODE_ENV=production THEMES=Main parallel-webpack --no-stats --max-retries=0",
    "test": "cross-env NPM_EXEPATH=$npm_exepath run-s test:*",
    "test:sasslint": "sass-lint -v -q",
    "test:eslint": "eslint --ext .js src/scripts",
    "test:flow": "flow check",
    "test:jest": "jest --coverage --runInBand"
  },
